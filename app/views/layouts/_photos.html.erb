<div class="row mt-4">
    <% @photos.each_slice(3) do |slice| %>
    <div class="col-lg-4 col-md-12 mb-4 mb-lg-0">

        <% slice.each do |photo| %>
        <%= image_tag photo['name'], loading: "lazy", class: "w-100 shadow-1-strong rounded mb-4" %>
        <% end %>

    </div>
    <% end %>
</div>
